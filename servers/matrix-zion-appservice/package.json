{
  "name": "matrix-zion-appservice",
  "description": "Bridge between Matrix and Zion smart contracts",
  "license": "MIT",
  "private": true,
  "version": "0.0.0",
  "main": "bin/app.js",
  "files": [
    "/bin"
  ],
  "scripts": {
    "install": "./install.sh",
    "build": "tsc",
    "start": "node ./bin/app.js -c config.yaml -p 6789",
    "dev": "ts-node ./src/app.ts -c config.yaml -p 6789",
    "register": "ts-node ./src/app.ts -r -f zion-appservice.yaml -c config.yaml -u http://localhost:6789",
    "watch": "tsc --watch"
  },
  "dependencies": {
    "@harmony/contracts": "workspace:^",
    "@types/node": "^17.0.32",
    "dotenv": "^16.0.1",
    "ethers": "^5.6.9",
    "express": "^4.18.1",
    "matrix-appservice-bridge": "^5.1.0",
    "matrix-bot-sdk": "^0.6.1",
    "typescript": "^4.6.4"
  },
  "devDependencies": {
    "ts-node": "^10.7.0"
  }
}
