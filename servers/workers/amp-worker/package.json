{
    "name": "amp-worker",
    "description": "Amplitude Reverse Proxy Worker",
    "version": "1.0.0",
    "author": "john@hntlabs.com",
    "license": "MIT OR Apache-2.0",
    "scripts": {
        "dev": "echo use dev:local for now - see wrangler.toml",
        "dev:local": "wrangler dev",
        "dev:remote": "wrangler dev --remote",
        "lint": "eslint .",
        "publish:prod": "wrangler deploy --env production",
        "publish:test": "wrangler deploy --env test",
        "publish:prod-beta": "wrangler deploy --env production-beta",
        "publish:test-beta": "wrangler deploy --env test-beta",
        "test": "yarn test:unit",
        "test:jest": "jest --coverage",
        "test:unit": "concurrently \"yarn test:jest\"",
        "test:wrangler": "concurrently --success command-1 --kill-others --kill-signal=SIGINT \"wrangler dev --local --port 8787\" \"wait-on --timeout 5000 http-get://localhost:8787/health\""
    },
    "dependencies": {
        "@towns/generated": "workspace:^",
        "ts-loader": "^9.4.2"
    },
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20230710.1",
        "@types/jest": "^29.4.0",
        "@typescript-eslint/eslint-plugin": "^5.61.0",
        "@typescript-eslint/parser": "^5.61.0",
        "concurrently": "^8.0.1",
        "eslint": "^8.42.0",
        "jest": "^29.6.2",
        "jest-environment-miniflare": "^2.14.0",
        "miniflare": "^3.20230710.0",
        "prettier": "^2.8.8",
        "ts-jest": "^29.1.1",
        "typescript": "^5.1.3",
        "wait-on": "^7.0.1",
        "wrangler": "^3.2.0"
    }
}
