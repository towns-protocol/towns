{
    "name": "amp-worker",
    "description": "Amplitude Reverse Proxy Worker",
    "version": "1.0.0",
    "author": "john@hntlabs.com",
    "license": "MIT OR Apache-2.0",
    "scripts": {
        "dev": "echo use dev:local for now - see wrangler.toml",
        "dev:local": "wrangler dev --local",
        "lint": "eslint .",
        "publish:prod": "wrangler publish --env production",
        "publish:stage": "wrangler publish --env staging",
        "publish:test": "wrangler publish --env test",
        "test": "yarn test:unit",
        "test:jest": "jest --coverage",
        "test:unit": "concurrently \"yarn test:jest\"",
        "test:wrangler": "concurrently --success command-1 --kill-others --kill-signal=SIGINT \"wrangler dev --local --port 8787\" \"wait-on --timeout 5000 http-get://localhost:8787/health\""
    },
    "dependencies": {
        "@harmony/generated": "workspace:^",
        "ts-loader": "^9.4.2"
    },
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20230419.0",
        "@types/jest": "^29.4.0",
        "@typescript-eslint/eslint-plugin": "^5.59.1",
        "@typescript-eslint/parser": "^5.59.1",
        "concurrently": "^8.0.1",
        "eslint": "^8.39.0",
        "jest": "^29.5.0",
        "jest-environment-miniflare": "^2.13.0",
        "miniflare": "^2.13.0",
        "prettier": "^2.8.8",
        "ts-jest": "^29.1.0",
        "typescript": "^5.0.4",
        "wait-on": "^7.0.1",
        "wrangler": "^2.19.0"
    }
}
