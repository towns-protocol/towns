# https://editorconfig.org
# Prettier & EditorConfig are adering to Wrangler codebase standards which
# are documented here: https://github.com/cloudflare/wrangler2/pull/1309
root = true

[*]
end_of_line = lf
insert_final_newline = true

[*.{js,json,yml}]
charset = utf-8
indent_style = space
indent_size = 2
