{
  "name": "push-notification-worker",
  "description": "Push Notification Worker",
  "version": "1.0.0",
  "author": "HNT Labs",
  "license": "MIT OR Apache-2.0",
  "private": true,
  "scripts": {
    "dev": "NO_D1_WARNING=true && wrangler dev",
    "dev:remote": "NO_D1_WARNING=true && wrangler dev --remote",
    "lint": "eslint .",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "publish:prod": "NO_D1_WARNING=true && wrangler deploy --env production",
    "publish:test": "NO_D1_WARNING=true && wrangler deploy --env test",
    "sqlcreate:local": "NO_D1_WARNING=true && wrangler d1 execute PushSubscription_test --local --file=./sql/create-schema.sql",
    "sqlverifyTags:local": "NO_D1_WARNING=true && wrangler d1 execute PushSubscription_test --local --command='select * from NotificationTag limit 20'",
    "sqlverifySubs:local": "NO_D1_WARNING=true && wrangler d1 execute PushSubscription_test --local --command='select * from PushSubscription limit 20'",
    "sqlcreate:test": "NO_D1_WARNING=true && wrangler d1 execute PushSubscription_test --env test --file=./sql/create-schema.sql",
    "sqlcreate:prod": "NO_D1_WARNING=true && wrangler d1 execute PushSubscription --env production --file=./sql/create-schema.sql",
    "test": "yarn test:unit --watch",
    "test:unit": "jest"
  },
  "dependencies": {
    "itty-router": "^2.6.6",
    "jsonwebtoken": "^9.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230710.1",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.3.0",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "better-sqlite3": "^8.4.0",
    "eslint": "^8.42.0",
    "jest": "^29.5.0",
    "jest-environment-miniflare": "^2.14.0",
    "jest-mock-extended": "^3.0.4",
    "miniflare": "^3.20230710.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3",
    "web-push": "^3.6.3",
    "wrangler": "^3.2.0"
  }
}
