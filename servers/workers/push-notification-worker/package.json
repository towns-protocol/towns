{
  "name": "push-notification-worker",
  "description": "Push Notification Worker",
  "version": "1.0.0",
  "author": "HNT Labs",
  "license": "MIT OR Apache-2.0",
  "private": true,
  "scripts": {
    "dev": "NO_D1_WARNING=true && wrangler dev",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "sqlcreate:local": "NO_D1_WARNING=true && wrangler d1 execute PushSubscription --local --file=./sql/create-schema-local.sql",
    "sqlverify:local": "NO_D1_WARNING=true && wrangler d1 execute PushSubscription --local --command='select * from PushSubscription limit 20'",
    "test": "yarn test:unit --watch",
    "test:unit": "jest"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230419.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^20.3.0",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "better-sqlite3": "^8.4.0",
    "eslint": "^8.42.0",
    "jest": "^29.5.0",
    "jest-environment-miniflare": "^2.13.0",
    "jest-mock-extended": "^3.0.4",
    "miniflare": "^2.13.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3",
    "wrangler": "^3.1.0"
  },
  "dependencies": {
    "itty-router": "^2.6.6"
  }
}
