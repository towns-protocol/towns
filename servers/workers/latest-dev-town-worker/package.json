{
	"name": "latest-dev-town-worker",
	"description": "A worker in charge of redirecting to the latest dev town",
	"version": "1.0.0",
	"author": "author",
	"license": "MIT OR Apache-2.0",
	"type": "module",
	"scripts": {
		"dev": "echo use dev:local for now - see wrangler.toml",
		"dev:local": "wrangler dev",
		"dev:remote": "wrangler dev --remote",
		"lint": "eslint .",
		"publish:test-beta": "wrangler deploy --env test-beta",
		"delete-latest-devtown-url:test-beta": "wrangler secret delete LATEST_DEV_TOWN_INVITE_URL --env test-beta",
		"set-latest-devtown-url:test-beta": "echo $LATEST_DEV_TOWN_INVITE_URL | wrangler secret put LATEST_DEV_TOWN_INVITE_URL --env test-beta",
		"test": "jest"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20230710.1",
		"@types/jest": "^29.5.3",
		"@typescript-eslint/eslint-plugin": "^6.10.0",
		"@typescript-eslint/parser": "^6.10.0",
		"eslint": "^8.53.0",
		"jest": "^29.6.2",
		"jest-environment-miniflare": "^2.14.0",
		"miniflare": "^3.20230710.0",
		"prettier": "^2.8.8",
		"ts-jest": "^29.1.1",
		"typescript": "^5.1.6",
		"wrangler": "^3.2.0"
	}
}
