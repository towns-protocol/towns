{
    "name": "stackup-worker",
    "description": "Stackup Middleware Worker",
    "version": "1.0.0",
    "author": "john@hntlabs.com",
    "license": "MIT OR Apache-2.0",
    "type": "module",
    "scripts": {
        "dev": "echo use dev:local for now - see wrangler.toml",
        "dev:local": "wrangler dev --port ${PORT:-8686}",
        "dev:remote": "wrangler dev --remote",
        "lint": "yarn eslint --format unix ./src",
        "prettier:check": "prettier --check .",
        "prettier:fix": "prettier --write .",
        "publish": "wrangler deploy --env $CF_ENV",
        "tail": "wrangler tail stackup-worker --env $CF_ENV",
        "test": "yarn test:unit",
        "test:smart-account": "vitest run --project smart-account",
        "test:unit": "vitest run --project router"
    },
    "dependencies": {
        "@privy-io/server-auth": "^1.9.1",
        "@towns-protocol/generated": "workspace:^",
        "@towns-protocol/web3": "workspace:^",
        "ethers": "^5.7.2",
        "itty-router": "^2.6.6",
        "permissionless": "^0.2.33",
        "userop.js": "^0.3.7",
        "viem": "2.23.2",
        "worker-common": "workspace:^"
    },
    "devDependencies": {
        "@cloudflare/vitest-pool-workers": "^0.7.5",
        "@cloudflare/workers-types": "^4.20240529.0",
        "@types/jest": "^29.5.3",
        "@typescript-eslint/eslint-plugin": "^8.29.0",
        "@typescript-eslint/parser": "^8.29.0",
        "eslint": "^8.57.0",
        "eslint-plugin-vitest": "^0.5.4",
        "miniflare": "^3.20240524.1",
        "prettier": "^2.8.8",
        "typescript": "^5.8.2",
        "vitest": "~3.0.7",
        "wrangler": "^4.6.0"
    }
}
