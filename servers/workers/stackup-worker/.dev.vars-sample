# add your secret variables here for local development
# for public variables, add them in wrangler.toml

ERC4337_ENTRYPOINT_ADDRESS=0x5FF137D4b0FDCD49DcA30c7CF57E578a026d2789

# STACKUP_PAYMASTER_RPC_URL is used for stackup's paymaster
# as of 8.14.2024, this only needs to be set for development against local stackup devnet
STACKUP_PAYMASTER_RPC_URL=

# skips all verification checks for an operation
SKIP_TOWNID_VERIFICATION=

# skips the limit verification for the operation (testing purposes only)
SKIP_LIMIT_VERIFICATION=

# skips the privy verification for the operation (testing purposes only)
SKIP_PRIVY_VERIFICATION=

# Authorization auth header token for requests, same as other workers
AUTH_SECRET=
# Authorization auth header token for /admin endpoints
ADMIN_AUTH_SECRET=

# required if SKIP_TOWNID_VERIFICATION is not true
PRIVY_APP_KEY=
# required if SKIP_TOWNID_VERIFICATION is not true
PRIVY_APP_ID=
# required if SKIP_TOWNID_VERIFICATION is not true
# needed for creating an ethers provider for limit checks
ALCHEMY_API_KEY=

# required if using Alchemy for userop sponsorship
# as of 8.14.2024, this is needed for production envs only
ALCHEMY_PAYMASTER_RPC_URL=
ALCHEMY_GM_POLICY_ID=

# development, test-beta, alpha, omega
ENVIRONMENT=development