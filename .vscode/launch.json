{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Jest: current file in 'web/lib/' (multi, legacy)",
      "type": "node",
      "request": "launch",
      "env": { "NODE_ENV": "development", "NODE_TLS_REJECT_UNAUTHORIZED": "0", "RIVER_ENV": "local_multi", "DEBUG": "csb:*", "DEBUG_DEPTH":"10" },
      "program": "${workspaceFolder}/node_modules/.bin/jest",
      "runtimeArgs": ["--experimental-vm-modules", "--experimental-wasm-modules"],
      "args": ["${file}", "--config", "${workspaceFolder}/clients/web/lib/jest.config.ts", "-i", "--no-cache", "--forceExit"],
      "console": "integratedTerminal",
    },
    {
      "name": "Jest: current file in 'web/lib/' (multi, v2)",
      "type": "node",
      "request": "launch",
      "env": { "NODE_ENV": "development", "NODE_TLS_REJECT_UNAUTHORIZED": "0", "CREATE_V2_SPACES": "true", "RIVER_ENV": "local_multi", "DEBUG": "csb:*", "DEBUG_DEPTH":"10" },
      "program": "${workspaceFolder}/node_modules/.bin/jest",
      "runtimeArgs": ["--experimental-vm-modules", "--experimental-wasm-modules"],
      "args": ["${file}", "--config", "${workspaceFolder}/clients/web/lib/jest.config.ts", "-i", "--no-cache", "--forceExit"],
      "console": "integratedTerminal",
    },
    {
      "name": "Launch App to Chrome",
      "url": "http://localhost:3000",
      "request": "launch",
      "type": "chrome",
      "preLaunchTask": "App",
      "webRoot": "${workspaceFolder}/clients/web/app",
      "userDataDir": "${workspaceFolder}/.vscode/vscode-chrome-debug-userdatadir"
    },
  ]
}
