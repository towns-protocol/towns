{
    "name": "@towns-protocol/bot-examples",
    "version": "0.0.167",
    "packageManager": "yarn@3.8.0",
    "type": "module",
    "scripts": {
        "dev:survey": "node esbuild.config.mjs && NODE_TLS_REJECT_UNAUTHORIZED=0 node --env-file=.env dist/survey-bot.cjs",
        "dev:thread-ai": "node esbuild.config.mjs && NODE_TLS_REJECT_UNAUTHORIZED=0 node --env-file=.env dist/thread-ai-bot.cjs",
        "clean": "rm -rf dist",
        "lint": "yarn eslint --format unix ./src  --max-warnings=0",
        "lint:fix": "yarn lint --fix"
    },
    "dependencies": {
        "@connectrpc/connect-node": "^2.0.0",
        "@hono/node-server": "^1.14.0",
        "@towns-protocol/bot": "workspace:^",
        "openai": "^4.95.1"
    },
    "devDependencies": {
        "@types/node": "^20.14.8",
        "@typescript-eslint/eslint-plugin": "^8.29.0",
        "@typescript-eslint/parser": "^8.29.0",
        "esbuild": "^0.21.5",
        "eslint": "^8.57.0",
        "eslint-import-resolver-typescript": "^3.5.5",
        "eslint-plugin-import": "^2.27.5",
        "typescript": "^5.8.2"
    },
    "files": [
        "/dist"
    ]
}
