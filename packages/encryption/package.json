{
    "name": "@towns-protocol/encryption",
    "version": "0.0.429",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./storage": {
            "types": "./dist/storage/index.d.ts",
            "import": "./dist/storage/index.js"
        }
    },
    "scripts": {
        "build": "tsc",
        "cb": "yarn clean && yarn build",
        "clean": "rm -rf dist",
        "lint": "yarn eslint --format unix ./src",
        "lint:fix": "yarn lint --fix",
        "test": "vitest run",
        "test:ci": "yarn test",
        "test:debug": "DEBUG=test yarn test",
        "test:watch": "vitest --watch",
        "watch": "tsc --watch"
    },
    "dependencies": {
        "@bufbuild/protobuf": "^2.9.0",
        "@towns-protocol/olm": "3.2.28",
        "@towns-protocol/proto": "workspace:^",
        "@towns-protocol/storage": "workspace:^",
        "@towns-protocol/utils": "workspace:^",
        "@towns-protocol/web3": "workspace:^",
        "debug": "^4.3.4",
        "dexie": "^4.2.1",
        "ethers": "^5.8.0",
        "lru-cache": "^11.0.1",
        "nanoid": "^4.0.0",
        "typed-emitter": "^2.1.0"
    },
    "devDependencies": {
        "@types/debug": "^4.1.8",
        "@types/node": "^20.14.8",
        "@typescript-eslint/eslint-plugin": "^8.29.0",
        "@typescript-eslint/parser": "^8.29.0",
        "eslint": "^8.57.1",
        "eslint-import-resolver-typescript": "^4.3.2",
        "eslint-plugin-import-x": "^4.10.2",
        "fake-indexeddb": "^6.0.1",
        "ts-node": "^10.9.1",
        "typescript": "~5.8.3",
        "vitest": "^3.2.3"
    },
    "files": [
        "dist"
    ],
    "publishConfig": {
        "access": "public"
    }
}
