{
    "name": "@towns-protocol/sdk",
    "version": "0.0.448",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc",
        "cb": "bun run clean && bun run build",
        "clean": "rm -rf dist",
        "lint": "eslint --format unix ./src  --max-warnings=0",
        "lint:fix": "bun run lint --fix",
        "test:ci": "bun run test:ci:multi:ne",
        "test:ci:multi:ent": "vitest run --silent=passed-only --config vitest.config.ts",
        "test:ci:multi:ent:legacy": "vitest run --silent=passed-only --config vitest.config.multi_legacy.ts",
        "test:ci:multi:ne": "vitest run --silent=passed-only --config vitest.config.multi_ne.ts",
        "test:ci:with-entitlements": "bun run test:ci:multi:ent",
        "test:unit": "vitest run --silent=passed-only --config vitest.config.unit.ts",
        "watch": "tsc --watch"
    },
    "dependencies": {
        "@bufbuild/protobuf": "^2.9.0",
        "@connectrpc/connect": "^2.1.0",
        "@ethereumjs/util": "^10.0.0",
        "@noble/curves": "^1.9.1",
        "@noble/hashes": "^1.8.0",
        "@towns-protocol/encryption": "workspace:^",
        "@towns-protocol/generated": "workspace:^",
        "@towns-protocol/proto": "workspace:^",
        "@towns-protocol/rpc-connector": "workspace:^",
        "@towns-protocol/sdk-crypto": "workspace:^",
        "@towns-protocol/utils": "workspace:^",
        "@towns-protocol/web3": "workspace:^",
        "debug": "^4.3.4",
        "dexie": "^4.2.1",
        "ethereum-cryptography": "^3.2.0",
        "ethers": "^5.8.0",
        "lodash-es": "^4.17.21",
        "nanoid": "^4.0.0",
        "p-limit": "^6.1.0"
    },
    "devDependencies": {
        "@connectrpc/connect-web": "^2.1.0",
        "@types/bun": "^1.3.1",
        "@types/debug": "^4.1.8",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^20.14.8",
        "@types/seedrandom": "^3.0.4",
        "@typescript-eslint/eslint-plugin": "^8.29.0",
        "@typescript-eslint/parser": "^8.29.0",
        "eslint": "^8.57.1",
        "eslint-import-resolver-typescript": "^4.3.2",
        "eslint-plugin-import-x": "^4.10.2",
        "eslint-plugin-tsdoc": "^0.3.0",
        "fake-indexeddb": "^6.0.1",
        "happy-dom": "^19.0.2",
        "seedrandom": "^3.0.5",
        "typed-emitter": "^2.1.0",
        "typescript": "~5.8.3",
        "vite-plugin-wasm": "^3.4.1",
        "vitest": "^3.2.3",
        "vitest-fetch-mock": "^0.4.3"
    },
    "files": [
        "/dist"
    ],
    "publishConfig": {
        "access": "public"
    }
}
