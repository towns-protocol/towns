{
    "name": "@towns-protocol/sdk",
    "version": "0.0.209",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc",
        "cb": "yarn clean && yarn build",
        "clean": "rm -rf dist",
        "dev": "my-node src/index.ts",
        "lint": "yarn eslint --format unix ./src  --max-warnings=0",
        "lint:fix": "yarn lint --fix",
        "test:ci": "yarn test:ci:multi:ne",
        "test:ci:multi:ent": "vitest run --config vitest.config.ts",
        "test:ci:multi:ent:legacy": "vitest run --config vitest.config.multi_legacy.ts",
        "test:ci:multi:ne": "vitest run --config vitest.config.multi_ne.ts",
        "test:ci:with-entitlements": "yarn test:ci:multi:ent",
        "test:unit": "vitest run --config vitest.config.unit.ts",
        "test:watch": "vitest --watch",
        "watch": "tsc --watch"
    },
    "dependencies": {
        "@bufbuild/protobuf": "^2.2.5",
        "@connectrpc/connect": "^2.0.2",
        "@ethereumjs/util": "^8.0.1",
        "@towns-protocol/dlog": "workspace:^",
        "@towns-protocol/encryption": "workspace:^",
        "@towns-protocol/generated": "workspace:^",
        "@towns-protocol/proto": "workspace:^",
        "@towns-protocol/web3": "workspace:^",
        "browser-or-node": "^3.0.0",
        "debug": "^4.3.4",
        "dexie": "^4.0.7",
        "ethereum-cryptography": "^1.2.0",
        "ethers": "^6.13.7",
        "lodash": "^4.17.21",
        "nanoid": "^4.0.0",
        "p-limit": "^6.1.0",
        "vitest": "3.0.5"
    },
    "devDependencies": {
        "@connectrpc/connect-web": "^2.0.0",
        "@types/debug": "^4.1.8",
        "@types/lodash": "^4.14.186",
        "@types/node": "^22.15.3",
        "@types/seedrandom": "^3.0.4",
        "@typescript-eslint/eslint-plugin": "^8.31.1",
        "@typescript-eslint/parser": "^8.31.1",
        "eslint": "^8.57.1",
        "eslint-import-resolver-typescript": "^4.3.2",
        "eslint-plugin-import-x": "^4.10.2",
        "eslint-plugin-tsdoc": "^0.3.0",
        "fake-indexeddb": "^4.0.1",
        "happy-dom": "^15.11.7",
        "ioredis": "^5.3.2",
        "msgpackr": "^1.10.1",
        "seedrandom": "^3.0.5",
        "typed-emitter": "^2.1.0",
        "typescript": "^5.8.2",
        "vite": "^6.3.4",
        "vite-plugin-wasm": "^3.4.1",
        "vitest-fetch-mock": "^0.4.3"
    },
    "files": [
        "/dist"
    ],
    "peerDependencies": {
        "@connectrpc/connect-node": "^2.0.0",
        "@connectrpc/connect-web": "^2.0.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
