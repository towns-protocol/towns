{
    "name": "@towns-protocol/sdk",
    "version": "0.0.236",
    "packageManager": "yarn@3.8.0",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "tsc",
        "cb": "yarn clean && yarn build",
        "clean": "rm -rf dist",
        "dev": "my-node src/index.ts",
        "lint": "yarn eslint --format unix ./src  --max-warnings=0",
        "lint:fix": "yarn lint --fix",
        "test:unit": "vitest run --config vitest.config.unit.ts",
        "test:ci": "yarn test:ci:multi:ne",
        "test:ci:multi:ent:legacy": "vitest run --config vitest.config.multi_legacy.ts",
        "test:ci:multi:ent": "vitest run --config vitest.config.ts",
        "test:ci:multi:ne": "vitest run --config vitest.config.multi_ne.ts",
        "test:ci:with-entitlements": "yarn test:ci:multi:ent",
        "test:watch": "vitest --watch",
        "watch": "tsc --watch"
    },
    "dependencies": {
        "@bufbuild/protobuf": "^2.5.1",
        "@connectrpc/connect": "^2.0.2",
        "@connectrpc/connect-node": "^2.0.2",
        "@connectrpc/connect-web": "^2.0.2",
        "@ethereumjs/util": "^8.1.0",
        "@noble/curves": "^1.9.1",
        "@noble/hashes": "^1.8.0",
        "@towns-protocol/dlog": "workspace:^",
        "@towns-protocol/encryption": "workspace:^",
        "@towns-protocol/generated": "workspace:^",
        "@towns-protocol/proto": "workspace:^",
        "@towns-protocol/web3": "workspace:^",
        "browser-or-node": "^3.0.0",
        "debug": "^4.4.1",
        "dexie": "^4.0.11",
        "ethereum-cryptography": "^3.2.0",
        "ethers": "^5.8.0",
        "lodash": "^4.17.21",
        "nanoid": "^4.0.2",
        "p-limit": "^6.2.0",
        "vitest": "3.0.5"
    },
    "devDependencies": {
        "@types/debug": "^4.1.12",
        "@types/lodash": "^4.17.17",
        "@types/node": "^20.17.57",
        "@types/seedrandom": "^3.0.8",
        "@typescript-eslint/eslint-plugin": "^8.33.1",
        "@typescript-eslint/parser": "^8.33.1",
        "eslint": "^8.57.1",
        "eslint-import-resolver-typescript": "^4.4.2",
        "eslint-plugin-import-x": "^4.15.0",
        "eslint-plugin-tsdoc": "^0.3.0",
        "fake-indexeddb": "^4.0.2",
        "happy-dom": "^15.11.7",
        "ioredis": "^5.6.1",
        "msgpackr": "^1.11.4",
        "seedrandom": "^3.0.5",
        "typed-emitter": "^2.1.0",
        "typescript": "^5.8.3",
        "vite-plugin-wasm": "^3.4.1",
        "vitest-fetch-mock": "^0.4.5"
    },
    "files": [
        "/dist"
    ]
}
