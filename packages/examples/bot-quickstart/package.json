    "devDependencies": {
        "@types/bun": "^1.3.1",
        "@types/node": "^20.14.8",
        "eslint-import-resolver-typescript": "^4.3.2",
        "eslint-plugin-import-x": "^4.10.2",
        "typescript": "~5.8.3"
    },
    "files": [
        "/dist"
    ]
