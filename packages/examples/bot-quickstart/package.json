{
    "name": "@towns-protocol/example-bot-quickstart",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "main": "src/index.ts",
    "scripts": {
        "build": "tsc --noEmit",
        "clean": "rm -rf dist",
        "dev": "bun run --watch src/index.ts",
        "lint": "eslint --format unix ./src  --max-warnings=0",
        "lint:fix": "eslint --format unix ./src  --max-warnings=0 --fix",
        "start": "bun run src/index.ts",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@connectrpc/connect-node": "^2.1.0",
        "@towns-protocol/bot": "workspace:^",
        "@towns-protocol/proto": "workspace:^",
        "@towns-protocol/web3": "workspace:^",
        "hono": "^4.7.11",
        "viem": "^2.29.3"
    },
    "devDependencies": {
        "@types/bun": "^1.3.1",
        "@types/node": "^20.14.8",
        "@typescript-eslint/eslint-plugin": "^8.29.0",
        "@typescript-eslint/parser": "^8.29.0",
        "eslint": "^8.57.1",
        "eslint-import-resolver-typescript": "^4.3.2",
        "eslint-plugin-import-x": "^4.10.2",
        "eslint-plugin-tsdoc": "^0.3.0",
        "typescript": "~5.8.3"
    },
    "files": [
        "/dist"
    ]
}
