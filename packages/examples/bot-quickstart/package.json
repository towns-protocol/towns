{
    "name": "@towns-protocol/example-bot-quickstart",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "main": "src/index.ts",
    "scripts": {
        "build": "tsc --noEmit",
        "clean": "rm -rf dist",
        "dev": "bun run --watch src/index.ts",
