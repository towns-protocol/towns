{
    "name": "@towns-protocol/dlog",
    "version": "0.0.273",
    "packageManager": "yarn@3.8.0",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "tsc",
        "cb": "yarn clean && yarn build",
        "clean": "rm -rf dist",
