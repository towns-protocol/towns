{
    "name": "@towns-protocol/stress",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "bs": "bun run build && node --enable-source-maps ./dist/simpleChat.cjs",
        "build": "bun run typecheck && bun run build-esbuild",
        "build-esbuild": "bun esbuild.config.mjs",
        "demo": "node --enable-source-maps ./dist/demo.cjs",
        "generate-wallets": "node --enable-source-maps ./dist/generateWallets.cjs",
        "lint": "eslint . --ext .ts",
        "nft": "node --enable-source-maps ./dist/nft.cjs",
        "notifications": "node --enable-source-maps ./dist/notifications.cjs",
        "start": "node --enable-source-maps ./dist/start.cjs",
        "test:ci:with-entitlements": "vitest run --config ./vitest.config.entitlements.ts --silent=passed-only",
        "test:stress": "vitest run --config ./vitest.config.entitlements.ts ./src/stress.test.ts",
        "test:stress:dev": "bun run test:stress | bun run pino-pretty",
        "transfer-funds": "node --enable-source-maps ./dist/transferFunds.cjs",
        "typecheck": "tsc --noEmit",
        "watch": "tsc --watch"
    },
    "dependencies": {
        "@babel/node": "^7.23.9",
        "@bufbuild/protobuf": "^2.9.0",
        "@connectrpc/connect-node": "^2.1.0",
        "@towns-protocol/encryption": "workspace:^",
        "@towns-protocol/olm": "3.2.28",
        "@towns-protocol/proto": "workspace:^",
        "@towns-protocol/rpc-connector": "workspace:^",
        "@towns-protocol/sdk": "workspace:^",
        "@towns-protocol/utils": "workspace:^",
        "@towns-protocol/web3": "workspace:^",
        "bullmq": "^5.43.1",
        "dotenv": "^17.2.3",
        "esbuild-plugin-pino": "^2.2.0",
        "ethers": "^5.8.0",
        "fake-indexeddb": "^6.0.1",
        "ioredis": "^5.3.2",
        "pino": "^9.0.0",
        "pino-pretty": "^13.0.0"
    },
    "devDependencies": {
        "@types/debug": "^4.1.8",
        "@types/node": "^20.14.8",
        "@typescript-eslint/eslint-plugin": "^8.29.0",
        "@typescript-eslint/parser": "^8.29.0",
        "esbuild": "^0.25.9",
        "eslint": "^8.57.1",
        "eslint-import-resolver-typescript": "^4.3.2",
        "eslint-plugin-import-x": "^4.10.2",
        "eslint-plugin-jest": "^27.2.3",
        "typed-emitter": "^2.1.0",
        "typescript": "~5.8.3",
        "vitest": "^3.2.3"
    },
    "files": [
        "/dist"
    ]
}
