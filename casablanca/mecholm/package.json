        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jest": "^27.2.1",
        "jest": "^29.5.0",
        "jest-extended": "^3.2.4",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.1",
