{
    "name": "@zion/proto",
    "private": true,
    "packageManager": "yarn@3.2.3",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "buf:clean": "rm -rf ./src/protocol_*.ts",
        "buf:generate": "buf generate --path protocol.proto",
        "buf:lint": "buf lint --path protocol.proto",
        "build": "tsc",
        "cb": "yarn clean && yarn build",
        "clean": "rm -rf ./dist",
        "my-jest": "yarn node -r ../../../scripts/node-no-warn.js --experimental-vm-modules $(yarn bin jest)",
        "test": "yarn my-jest",
        "test:ci": "yarn my-jest"
    },
    "dependencies": {
        "@bufbuild/connect-web": "^0.6.0",
        "@bufbuild/protobuf": "^1.0.0"
    },
    "devDependencies": {
        "@bufbuild/protoc-gen-connect-web": "^0.6.0",
        "@bufbuild/protoc-gen-es": "^1.0.0",
        "@types/jest": "^29.4.0",
        "jest": "^29.4.3",
        "jest-extended": "^3.2.0",
        "ts-jest": "^29.0.5",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5"
    },
    "files": [
        "/dist"
    ]
}
