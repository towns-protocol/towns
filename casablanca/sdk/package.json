{
    "name": "@towns/sdk",
    "packageManager": "yarn@3.2.3",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc",
        "cb": "yarn clean && yarn build",
        "clean": "rm -rf dist",
        "dev": "my-node src/index.ts",
        "lint": "yarn eslint --format unix ./src",
        "lint:fix": "yarn lint --fix",
        "my-jest": "yarn node -r ../../scripts/node-no-warn.js --experimental-vm-modules $(yarn bin jest)",
        "my-node": "yarn node -r ../../scripts/node-no-warn.js --loader ts-node/esm --experimental-specifier-resolution=node --experimental-vm-modules",
        "test": "yarn my-jest",
        "test:ci": "yarn my-jest --testTimeout=180000",
        "watch": "tsc --watch"
    },
    "dependencies": {
        "@bufbuild/connect": "^0.9.1",
        "@bufbuild/connect-web": "^0.9.1",
        "@bufbuild/protobuf": "^1.2.1",
        "@ethereumjs/util": "^8.0.1",
        "@towns/mecholm": "workspace:^",
        "@towns/proto": "workspace:^",
        "debug": "^4.3.4",
        "ethereum-cryptography": "1.2.0",
        "ethers": "^5.7.2",
        "lodash": "^4.17.21",
        "nanoid": "^4.0.0",
        "supports-color": "^9.2.3"
    },
    "devDependencies": {
        "@jest/globals": "^29.5.0",
        "@types/jest": "^29.4.0",
        "@types/lodash": "^4.14.186",
        "@types/node": "^18.14.2",
        "@types/seedrandom": "^3.0.4",
        "@typescript-eslint/eslint-plugin": "^5.59.1",
        "@typescript-eslint/parser": "^5.59.1",
        "eslint": "^8.39.0",
        "eslint-import-resolver-typescript": "^3.5.5",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jest": "^27.2.1",
        "jest": "^29.5.0",
        "jest-extended": "^3.2.4",
        "seedrandom": "^3.0.5",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.1",
        "typed-emitter": "^2.1.0",
        "typescript": "^5.1.3"
    },
    "files": [
        "/dist"
    ],
    "resolutions": {
        "jest-worker@^29.5.0": "patch:jest-worker@npm%3A29.5.0#./.yarn/patches/jest-worker-npm-29.5.0-70da3388f1.patch"
    }
}
