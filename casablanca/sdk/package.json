{
    "name": "@river/sdk",
    "packageManager": "yarn@3.2.3",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc",
        "cb": "yarn clean && yarn build",
        "clean": "rm -rf dist",
        "dev": "my-node src/index.ts",
        "lint": "yarn eslint --format unix ./src  --max-warnings=0",
        "lint:fix": "yarn lint --fix",
        "my-jest": "yarn node -r ../../scripts/node-no-warn.js --experimental-vm-modules $(yarn bin jest)",
        "my-node": "yarn node -r ../../scripts/node-no-warn.js --loader ts-node/esm --experimental-specifier-resolution=node --experimental-vm-modules",
        "test": "RIVER_TEST_CONNECT=single yarn test:base",
        "test:base": "yarn my-jest --group=-node-minipool-flush --group=-with-entitilements --group=-restart --group=-synthetic --group=-load-tests-s2 --group=-load-tests-s3",
        "test:base:ent": "yarn my-jest --group=with-entitilements --group=-synthetic --group=-load-tests-s2 --group=-load-tests-s3",
        "test:ci": "yarn test --testTimeout=180000",
        "test:ci:load-tests-s2": "RIVER_TEST_CONNECT=single yarn my-jest --group=load-tests-s2 --testTimeout=1180000",
        "test:ci:load-tests-s3": "RIVER_TEST_CONNECT=single yarn my-jest --group=load-tests-s3 --testTimeout=1180000",
        "test:ci:with-entitlements": "RIVER_TEST_CONNECT=single_ent yarn test:base:ent",
        "test:ci:synthetic:all": "RIVER_TEST_CONNECT=single yarn my-jest --group=synthetic --testTimeout=180000",
        "test:ci:multi": "RIVER_TEST_CONNECT=multi yarn test:base",
        "test:ci:multi:ent": "RIVER_TEST_CONNECT=multi yarn test:base:ent",
        "test:node-minipool-flush": "RIVER_TEST_CONNECT=single yarn my-jest --group=node-minipool-flush --numWorkers=1",
        "test:restart": "RIVER_TEST_CONNECT=single yarn my-jest --group=restart --numWorkers=1",
        "watch": "tsc --watch"
    },
    "dependencies": {
        "@bufbuild/protobuf": "^1.4.1",
        "@connectrpc/connect": "^1.1.3",
        "@connectrpc/connect-web": "^1.1.3",
        "@ethereumjs/util": "^8.0.1",
        "@river/mecholm": "workspace:^",
        "@river/proto": "workspace:^",
        "@river/web3": "workspace:^",
        "browser-or-node": "^2.1.1",
        "debug": "^4.3.4",
        "dexie": "^3.2.4",
        "ethereum-cryptography": "1.2.0",
        "ethers": "^5.7.2",
        "lodash": "^4.17.21",
        "nanoid": "^4.0.0",
        "supports-color": "^9.2.3"
    },
    "devDependencies": {
        "@jest/globals": "^29.6.2",
        "@types/debug": "^4.1.8",
        "@types/jest": "^29.5.3",
        "@types/lodash": "^4.14.186",
        "@types/node": "^20.5.0",
        "@types/seedrandom": "^3.0.4",
        "@typescript-eslint/eslint-plugin": "^6.10.0",
        "@typescript-eslint/parser": "^6.10.0",
        "bullmq": "^4.10.0",
        "eslint": "^8.53.0",
        "eslint-import-resolver-typescript": "^3.5.5",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jest": "^27.2.3",
        "fake-indexeddb": "^4.0.1",
        "jest": "^29.6.2",
        "jest-extended": "^4.0.1",
        "jest-mock-extended": "^3.0.4",
        "jest-runner-groups": "^2.2.0",
        "seedrandom": "^3.0.5",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "typed-emitter": "^2.1.0",
        "typescript": "^5.1.6"
    },
    "files": [
        "/dist"
    ],
    "resolutions": {
        "jest-worker@^29.5.0": "patch:jest-worker@npm%3A29.5.0#./.yarn/patches/jest-worker-npm-29.5.0-70da3388f1.patch"
    }
}
