{
    "name": "@towns/proto",
    "private": true,
    "packageManager": "yarn@3.2.3",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "buf:clean": "rm -rf ./src/gen/*",
        "buf:generate": "buf generate --path protocol.proto && buf generate --path payloads.proto",
        "buf:lint": "buf lint --path protocol.proto && buf lint --path payloads.proto",
        "build": "buf generate --path protocol.proto && buf generate --path payloads.proto && tsc ",
        "cb": "yarn clean && yarn build",
        "clean": "rm -rf ./dist",
        "my-jest": "yarn node -r ../../scripts/node-no-warn.js --experimental-vm-modules $(yarn bin jest)",
        "postinstall": "yarn build",
        "test": "yarn my-jest",
        "test:ci": "yarn my-jest",
        "watch": "node scripts/watch_proto_files.cjs",
        "watch:go": "node scripts/watch_proto_files_go.cjs"
    },
    "dependencies": {
        "@bufbuild/connect": "^0.9.1",
        "@bufbuild/connect-web": "^0.9.1",
        "@bufbuild/protobuf": "^1.2.1"
    },
    "devDependencies": {
        "@bufbuild/buf": "^1.21.0-1",
        "@bufbuild/protoc-gen-connect-es": "^0.9.1",
        "@bufbuild/protoc-gen-es": "^1.2.1",
        "@types/jest": "^29.4.0",
        "jest": "^29.5.0",
        "jest-extended": "^3.2.4",
        "lodash.debounce": "^4.0.8",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.3"
    },
    "files": [
        "/dist"
    ]
}
